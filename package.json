{
  "name": "pi-validator",
  "version": "1.0.0",
  "description": "Dashboard expanssion for PI",
  "repository": "https://github.com/ProjectImplicit/pi-validator.git",
  "main": "route.js",
  "scripts": {
    "start": "nodemon server/index --watch server",
    "setup": "node server/setup",
    "reset_admin": "node server/reset_admin",
    "reset_ssl": "node server/reset_ssl",

    "test": "eslint .",
    "js": "rollup --config --sourcemap",
    "css": "node-sass dashboard/src/style/style.scss --output dashboard/dist --source-map=true",
    "bundle": "npm-run-all --parallel js css",
    "watch:js": "rollup --config --sourcemap --watch",
    "watch:css": "node-sass -w dashboard/src/style --output dashboard/dist --source-map=true",
    "watch": "npm-run-all --parallel watch:*",
    "image": "docker build -t minnoserver .",
    "dockerup": "docker-compose up",
    "dev": "node --trace-warnings --inspect route.js",
    "version": "npm-run-all minify css && git add -A dist",
    "minify": "rollup --config --environment NODE_ENV:production"
  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "archiver": "^2.1.1",
    "archiver-promise": "^1.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "csv-stringify": "^3.1.1",
    "dateformat": "^3.0.3",
    "dotenv": "^6.2.0",
    "ejs": "^2.7.1",
    "email-validator": "^1.2.3",
    "express": "^4.17.1",
    "express-fingerprint": "^1.1.3",
    "express-session": "^1.16.2",
    "fetch": "^1.1.0",
    "formidable": "^1.2.1",
    "fs-extra": "^5.0.0",
    "get-ssl-certificate": "^2.3.3",
    "greenlock": "^2.8.6",
    "greenlock-express": "^2.7.16",
    "greenlock-store-fs": "^3.0.2",
    "mongoose": "^5.6.12",
    "mongoose_schema-json": "^1.0.3",
    "multer": "^1.4.2",
    "node-dropbox": "^0.1.8",
    "node-fetch": "^2.6.0",
    "node-forge": "^0.8.5",
    "node-input-validator": "^2.3.4",
    "nodemailer": "^6.3.0",
    "nodemailer-promise": "^2.0.0",
    "nodemon": "^1.19.2",
    "promisify-node": "^0.5.0",
    "request-promise": "^4.2.4",
    "sanitize-filename": "^1.6.3",
    "simple-node-logger": "^0.93.42",
    "spdy": "^3.4.7",
    "ssl-checker": "^1.2.1",
    "static-server": "^2.2.1",
    "through2": "^2.0.5",
    "urlencode": "^1.1.0",
    "valid-filename": "^2.0.1",
    "walkdir": "0.0.12",
    "zip-a-folder": "0.0.8",
    "zip-dir": "^1.0.2"
  },
  "devDependencies": {
    "buble": "^0.12.3",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.18.2",
    "mithril": "^0.2.8",
    "node-sass": "^3.13.1",
    "npm-run-all": "^2.3.0",
    "pikaday": "^1.4.0",
    "promise-polyfill": "^8.1.3",
    "rollup": "^0.67.1",
    "rollup-plugin-buble": "^0.12.1",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-includepaths": "^0.2.3",
    "rollup-plugin-node-resolve": "^4.2.4",
    "rollup-plugin-uglify": "^6.0.3",
    "rollup-watch": "^2.4.0",
    "url-join": "^4.0.1",
    "whatwg-fetch": "^3.0.0"
  }
}

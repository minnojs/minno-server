{
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
    "indent": [1, 4, {"SwitchCase": 1}],
        "quotes": [1,"single"],
        "linebreak-style": [2,"unix"],
        "semi": [1,"always"],
        "no-console": [1],
        "no-var": [1]
    },
    "env": {
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended"
}
